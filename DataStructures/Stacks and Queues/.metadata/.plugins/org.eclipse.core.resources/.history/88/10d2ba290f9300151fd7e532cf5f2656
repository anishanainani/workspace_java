public class Stack<Element>{
	Node<Element> top;
	int size;
	
	public Stack(){
		top = null;
		size = 0;
	}
	public void push(Element elem){
		Node<Element> n = new Node<Element>();
		n.data = elem;
		n.next = top;
		top = n;
		size++;
	}

	public Element pop(){
		if(top != null){
			Element temp = top.data;
			top=top.next;
			size--;
			return temp;
		}

		return null;
	}
	
	public void displayAll(){
		Node<Element> elem = top;
		while(elem != null){
			System.out.println(elem.data);
			elem=elem.next;
		}
		
	}
}
