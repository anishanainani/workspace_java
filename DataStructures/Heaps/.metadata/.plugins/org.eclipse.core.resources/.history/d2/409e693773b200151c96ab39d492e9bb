import java.util.Scanner;


public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		int n = cin.nextInt();
		
		BinaryHeap bh = new BinaryHeap();
		for(int i=1; i<=n; ++i){
			bh.heap_insert(cin.nextInt());
		}
		
		
	
	}
	
	

}
